
### USER ALIASES:
alias e='exit'
alias edit='vim ~/.bash_aliases && source ~/.bash_aliases && echo .bash_aliases sourced'
alias desk='cd ~/Desktop'
alias minwin='gsettings set org.compiz.unityshell:/org/compiz/profiles/unity/plugins/unityshell/ launcher-minimize-window true'
alias upall='sudo apt-get update && sudo apt-get upgrade'
alias get-clion='sudo add-apt-repository ppa:ubuntu-desktop/ubuntu-make && sudo apt-get update && sudo apt-get install ubuntu-make && umake ide clion'
alias repo='cd ~/Desktop/git-repos'

